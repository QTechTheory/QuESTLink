Cell[BoxData[
 RowBox[{
  RowBox[{"myDevSpec", " ", "=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"t", ",", " ", "\[CapitalDelta]t"}], "}"}], ",", "\n", 
     RowBox[{"<|", "\n", "\t", 
      RowBox[{
       RowBox[{
       "DeviceDescription", " ", "\[Rule]", " ", 
        "\"\<A line of qubits with progressively worsening and slowing gates. \
Better act quick!\>\""}], ",", "\n", "\t", 
       RowBox[{"NumAccessibleQubits", " ", "\[Rule]", " ", "5"}], ",", "\n", 
       "\t", 
       RowBox[{"NumTotalQubits", " ", "\[Rule]", " ", "5"}], ",", "\n", "\t", 
       "\n", "\t", 
       RowBox[{"TimeSymbol", " ", "\[Rule]", " ", "t"}], ",", "\n", "\t", 
       RowBox[{"DurationSymbol", " ", "\[Rule]", " ", "\[CapitalDelta]t"}], 
       ",", "\n", "\n", "\t", 
       RowBox[{"Gates", " ", "\[Rule]", " ", 
        RowBox[{"{", "\n", "\t", "\n", "\t\t", 
         RowBox[{
          RowBox[{
           RowBox[{
            SubscriptBox["Rx", "q_"], "[", "\[Theta]_", "]"}], " ", 
           "\[RuleDelayed]", " ", 
           RowBox[{"<|", "\n", "\n", "\t\t\t", 
            RowBox[{"(*", " ", 
             RowBox[{
              RowBox[{"Over", "-", 
               RowBox[{
               "rotation", " ", "increasing", " ", "with", " ", "time"}]}], 
              ",", " ", 
              RowBox[{"and", " ", "gate", " ", "duration"}]}], " ", "*)"}], 
            "\n", "\t\t\t", 
            RowBox[{
             RowBox[{"NoisyForm", " ", "\[Rule]", " ", 
              RowBox[{"Circuit", "[", " ", 
               RowBox[{
                SubscriptBox["Rx", "q"], "[", 
                RowBox[{"\[Theta]", " ", "+", " ", 
                 RowBox[{"t", " ", "\[CapitalDelta]t", " ", 
                  RowBox[{"\[Pi]", "/", "10"}]}]}], "]"}], " ", "]"}]}], ",", 
             "\n", "\t\t\t", "\n", "\t\t\t", 
             RowBox[{"(*", " ", 
              RowBox[{
              "Meanwhile", " ", "the", " ", "gate", " ", "slows", " ", "down",
                " ", "quadratically", " ", "with", " ", 
               RowBox[{"time", "!"}]}], " ", "*)"}], "\n", "\t\t\t", 
             RowBox[{"GateDuration", " ", "\[Rule]", " ", 
              RowBox[{
               SuperscriptBox["t", "2"], " ", "+", " ", 
               RowBox[{
                RowBox[{"Abs", "[", "\[Theta]", "]"}], "/", 
                RowBox[{"(", 
                 RowBox[{"2", "\[Pi]"}], ")"}]}]}]}]}], "\n", "\t\t", 
            "|>"}]}], ",", "\n", "\t\t", "\n", "\t\t", 
          RowBox[{
           RowBox[{
            RowBox[{
             SubscriptBox["C", "c_"], "[", 
             SubscriptBox["X", "q_"], "]"}], " ", "/;", " ", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"Abs", "[", 
               RowBox[{"q", "-", "c"}], "]"}], " ", "===", " ", "1"}], 
             ")"}]}], " ", "\[RuleDelayed]", " ", 
           RowBox[{"<|", "\n", "\t\t", "\n", "\t\t\t", 
            RowBox[{"(*", " ", 
             RowBox[{
             "Active", " ", "noise", " ", "strength", " ", "worsens", " ", 
              "in", " ", "time"}], " ", "*)"}], "\n", "\t\t\t", 
            RowBox[{
             RowBox[{"NoisyForm", " ", "\[Rule]", " ", 
              RowBox[{"Circuit", "[", " ", 
               RowBox[{
                RowBox[{
                 SubscriptBox["C", "c"], "[", 
                 SubscriptBox["X", "q"], "]"}], " ", 
                RowBox[{
                 SubscriptBox["Depol", 
                  RowBox[{"c", ",", "q"}]], "[", 
                 RowBox[{".5", " ", "-", " ", 
                  RowBox[{".5", 
                   RowBox[{"(", 
                    RowBox[{"1", "-", 
                    SuperscriptBox["\[ExponentialE]", 
                    RowBox[{"-", "t"}]]}], ")"}]}]}], "]"}]}], " ", "]"}]}], 
             ",", "\n", "\t\t\t", "\n", "\t\t\t", 
             RowBox[{"GateDuration", " ", "\[Rule]", " ", 
              RowBox[{"1", " ", "+", " ", "q"}]}]}], "\n", "\t\t", "|>"}]}]}],
          "\n", "\t", "}"}]}], ",", "\n", "\t", "\n", "\t", 
       RowBox[{"Qubits", " ", "\[Rule]", " ", 
        RowBox[{"{", "\n", "\t\t", 
         RowBox[{"(*", " ", 
          RowBox[{
          "All", " ", "passive", " ", "noise", " ", "depends", " ", "both", 
           " ", "on", " ", "start", " ", "time", " ", "of", " ", "qubit", " ",
            "idleness", " ", 
           RowBox[{"(", 
