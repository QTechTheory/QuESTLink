                "]"}]}], ",", "\n", "\t\t\t", 
              RowBox[{"GateDuration", " ", "\[Rule]", " ", 
               RowBox[{"1", " ", "+", " ", "q"}]}]}], "\n", "\t\t", 
             "|>"}]}]}], "\n", "\t", "}"}]}], ",", "\n", "\t", "\n", "\t", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Declare", " ", "that", " ", "\[CapitalDelta]t", " ", "will", " ", 
          "refer", " ", "to", " ", "the", " ", "duration", " ", "of", " ", 
          "the", " ", "current", " ", 
          RowBox[{"gate", "/", 
           RowBox[{"channel", "."}]}]}], " ", "*)"}], "\n", "\t", 
        RowBox[{"DurationSymbol", " ", "\[Rule]", " ", "\[CapitalDelta]t"}], 
        ",", "\n", "\t", "\n", "\t", 
        RowBox[{"Qubits", " ", "\[Rule]", " ", 
         RowBox[{"{", "\n", "\n", "\t\t", 
          RowBox[{"(*", " ", 
           RowBox[{
            RowBox[{"The", " ", "central", " ", "qubit", " ", 
             RowBox[{"(", 
              RowBox[{"index", " ", "2"}], ")"}], " ", "will", " ", 
             "depolarise"}], ",", " ", 
            RowBox[{
            "when", " ", "idle", " ", "for", " ", "\[CapitalDelta]t"}]}], " ",
            "*)"}], "\n", "\t\t", 
          RowBox[{
           RowBox[{"2", " ", "\[RuleDelayed]", " ", 
            RowBox[{"<|", "\n", "\t\t\t", 
             RowBox[{"PassiveNoise", " ", "\[Rule]", " ", 
              RowBox[{"Circuit", "[", " ", 
               RowBox[{
                SubscriptBox["Depol", "2"], "[", 
                RowBox[{".5", 
                 RowBox[{"(", 
                  RowBox[{"1", "-", 
                   SuperscriptBox["\[ExponentialE]", 
                    RowBox[{"-", "\[CapitalDelta]t"}]]}], ")"}]}], "]"}], 
               "]"}]}], "\n", "\t\t", "|>"}]}], ",", "\n", "\n", "\t\t", 
           RowBox[{"(*", " ", 
            RowBox[{
             RowBox[{"The", " ", "other", " ", "qubits", " ", 
              RowBox[{"(", 
               RowBox[{"except", " ", "3"}], ")"}], " ", "dephase"}], ",", 
             " ", 
             RowBox[{
             "with", " ", "strength", " ", "growing", " ", "quadratically", 
              " ", "with", " ", "qubit", " ", 
              RowBox[{"index", "!"}]}]}], " ", "*)"}], "\n", "\t\t", 
           RowBox[{
            RowBox[{"q_", " ", "/;", " ", 
             RowBox[{"(", 
              RowBox[{"q", " ", "=!=", " ", "3"}], ")"}]}], " ", 
            "\[RuleDelayed]", " ", 
            RowBox[{"<|", "\n", "\t\t\t", 
             RowBox[{"PassiveNoise", " ", "\[Rule]", " ", 
              RowBox[{"With", "[", "\n", "\t\t\t\t", 
               RowBox[{
                RowBox[{"{", 
                 RowBox[{"f", " ", "=", " ", 
                  RowBox[{
                   SuperscriptBox[
                    RowBox[{"(", 
                    RowBox[{"1", "+", "q"}], ")"}], "2"], 
                   RowBox[{"(", 
                    RowBox[{"1", "-", 
                    SuperscriptBox["\[ExponentialE]", 
                    RowBox[{"-", "\[CapitalDelta]t"}]]}], ")"}]}]}], "}"}], 
                ",", "\n", "\t\t\t\t", 
                RowBox[{"Circuit", "[", " ", 
                 RowBox[{
                  SubscriptBox["Deph", "q"], "[", "f", "]"}], " ", "]"}]}], 
               "]"}]}], " ", "\n", "\t\t", "|>"}]}]}], "\n", "\t", "}"}]}]}], 
       "\n", "|>"}]}], "]"}]}], ";"}]}]], "Code",
