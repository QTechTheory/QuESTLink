       RowBox[{
        RowBox[{
        "DeviceDescription", " ", "\[Rule]", " ", 
         "\"\<A line of 5 nearest-neighbour decohering qubits, with imperfect \
gates\>\""}], ",", "\n", "\t", 
        RowBox[{"NumAccessibleQubits", " ", "\[Rule]", " ", "5"}], ",", "\n", 
        "\t", 
        RowBox[{"NumTotalQubits", " ", "\[Rule]", " ", "5"}], ",", "\n", "\n",
         "\t", 
        RowBox[{"Gates", " ", "\[Rule]", " ", 
         RowBox[{"{", "\n", "\t\t", 
          RowBox[{
           RowBox[{
            RowBox[{
             SubscriptBox["Rx", "q_"], "[", "\[Theta]_", "]"}], " ", 
            "\[RuleDelayed]", " ", 
            RowBox[{"<|", "\n", "\t\t\t", 
             RowBox[{
              RowBox[{"NoisyForm", " ", "\[Rule]", " ", 
               RowBox[{"Circuit", "[", " ", 
                RowBox[{
                 SubscriptBox["Rx", "q"], "[", 
                 RowBox[{"\[Theta]", " ", "+", " ", 
                  RowBox[{"\[Pi]", "/", "10"}]}], "]"}], " ", "]"}]}], ",", 
              "\n", "\t\t\t", 
              RowBox[{"GateDuration", " ", "\[Rule]", " ", 
               RowBox[{
                RowBox[{"Abs", "[", "\[Theta]", "]"}], "/", 
                RowBox[{"(", 
                 RowBox[{"2", "\[Pi]"}], ")"}]}]}]}], "\n", "\t\t", "|>"}]}], 
           ",", "\n", "\t\t", 
           RowBox[{
            RowBox[{
             RowBox[{
              SubscriptBox["C", "c_"], "[", 
              SubscriptBox["X", "q_"], "]"}], " ", "/;", " ", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"Abs", "[", 
                RowBox[{"q", "-", "c"}], "]"}], " ", "===", " ", "1"}], 
              ")"}]}], " ", "\[RuleDelayed]", " ", 
            RowBox[{"<|", "\n", "\t\t\t", 
             RowBox[{
              RowBox[{"NoisyForm", " ", "\[Rule]", " ", 
               RowBox[{"Circuit", "[", " ", 
                RowBox[{
                 RowBox[{
