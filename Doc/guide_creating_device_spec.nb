Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Declare", " ", "\[CapitalDelta]t", " ", "a", " ", "private", " ", 
    "symbol", " ", "using", " ", 
    RowBox[{"Module", "[", "]"}]}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{"myDevSpec", " ", "=", " ", 
