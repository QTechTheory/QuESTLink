Cell[BoxData[
 RowBox[{
  RowBox[{"myDevSpec", " ", "=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "\n", "\t", 
      RowBox[{
      "\[CapitalDelta]t", ",", " ", "\n", "\t", "totalRotation", ",", " ", 
       "totalIdleTime", ",", " ", "numGatesOnQubit"}], "\n", "}"}], ",", " ", 
     
     RowBox[{"<|", "\n", "\t", 
      RowBox[{
       RowBox[{
       "DeviceDescription", " ", "\[Rule]", " ", 
        "\"\<Whacky device where Rx slow themselves down, idle qubits ruin T \
gates, and gates ruin idle qubits.\>\""}], ",", "\n", "\t", 
       RowBox[{"NumAccessibleQubits", " ", "\[Rule]", " ", "5"}], ",", "\n", 
       "\t", 
       RowBox[{"NumTotalQubits", " ", "\[Rule]", " ", "5"}], ",", "\n", "\t", 
       "\n", "\t", 
       RowBox[{"InitVariables", " ", "\[Rule]", " ", 
        RowBox[{"Function", "[", "\n", "\t\t", 
         RowBox[{
          RowBox[{"totalRotation", " ", "=", " ", "0"}], ";", "\n", "\t\t", 
          RowBox[{"totalIdleTime", " ", "=", " ", "0"}], ";", "\n", "\t\t", 
          RowBox[{"numGatesOnQubit", " ", "=", " ", 
           RowBox[{"ConstantArray", "[", 
            RowBox[{"0", ",", " ", "5"}], "]"}]}], ";"}], "\n", "\t", "]"}]}],
        ",", "\n", "\n", "\t", 
       RowBox[{"Gates", " ", "\[Rule]", " ", 
        RowBox[{"{", "\n", "\t\t", 
         RowBox[{
          RowBox[{
           SubscriptBox["T", "q_"], " ", "\[RuleDelayed]", " ", 
           RowBox[{"<|", "\n", "\t\t\t", 
            RowBox[{"(*", " ", 
             RowBox[{
             "Dephase", " ", "target", " ", "qubit", " ", "based", " ", "on", 
              " ", "total", " ", "cumulative", " ", "idle", " ", "time", " ", 
              "of", " ", "all", " ", "qubits", " ", "so", " ", "far"}], " ", 
             "*)"}], "\n", "\t\t\t", 
            RowBox[{
             RowBox[{"NoisyForm", " ", "\[Rule]", " ", 
              RowBox[{"Circuit", "[", " ", 
               RowBox[{
                SubscriptBox["T", "q"], " ", 
                RowBox[{
                 SubscriptBox["Deph", "q"], "[", "totalIdleTime", "]"}]}], 
               " ", "]"}]}], ",", "\n", "\t\t\t", 
             RowBox[{"GateDuration", " ", "\[Rule]", " ", "1"}], ",", "\n", 
             "\t\t\t", 
             RowBox[{"UpdateVariables", " ", "\[Rule]", " ", 
              RowBox[{"Function", "[", " ", 
               RowBox[{
                RowBox[{"numGatesOnQubit", "\[LeftDoubleBracket]", 
                 RowBox[{"q", "+", "1"}], "\[RightDoubleBracket]"}], " ", 
                "++"}], " ", "]"}]}]}], "\n", "\t\t", "|>"}]}], ",", "\n", 
          "\t\t", 
          RowBox[{
           RowBox[{
            SubscriptBox["Rx", "q_"], "[", "\[Theta]_", "]"}], " ", 
           "\[RuleDelayed]", " ", 
           RowBox[{"<|", "\n", "\t\t\t", 
            RowBox[{"(*", " ", 
             RowBox[{
             "Slow", " ", "Rx", " ", "by", " ", "total", " ", "rotation", " ",
               "so", " ", "far"}], " ", "*)"}], "\n", "\t\t\t", 
            RowBox[{
             RowBox[{"NoisyForm", " ", "\[Rule]", " ", 
              RowBox[{"Circuit", "[", " ", 
               RowBox[{
                SubscriptBox["Rx", "q"], "[", "\[Theta]", "]"}], " ", "]"}]}],
              ",", "\n", "\t\t\t", 
             RowBox[{"GateDuration", " ", "\[Rule]", " ", "totalRotation"}], 
             ",", "\n", "\t\t\t", 
             RowBox[{"UpdateVariables", " ", "\[Rule]", " ", 
              RowBox[{"Function", "[", "\n", "\t\t\t\t", 
               RowBox[{
                RowBox[{"totalRotation", " ", "+=", " ", 
                 RowBox[{"Abs", "[", "\[Theta]", "]"}]}], ";", "\n", 
                "\t\t\t\t", 
                RowBox[{
                 RowBox[{"numGatesOnQubit", "\[LeftDoubleBracket]", 
                  RowBox[{"q", "+", "1"}], "\[RightDoubleBracket]"}], " ", 
                 "++"}]}], "\n", "\t\t\t", "]"}]}]}], "\n", "\t\t", 
            "|>"}]}]}], "\n", "\t", "}"}]}], ",", "\n", "\t", "\n", "\t", 
       RowBox[{"DurationSymbol", " ", "\[Rule]", " ", "\[CapitalDelta]t"}], 
       ",", "\n", "\t", 
       RowBox[{"Qubits", " ", "\[Rule]", " ", 
        RowBox[{"{", "\n", "\t\t", 
         RowBox[{"q_", " ", "\[RuleDelayed]", " ", 
          RowBox[{"<|", "\n", "\t\t\t", 
           RowBox[{"(*", " ", 
            RowBox[{
            "Dampen", " ", "idle", " ", "qubit", " ", "based", " ", "on", " ",
              "the", " ", "number", " ", "of", " ", "gates", " ", "applied", 
             " ", "to", " ", "it", " ", "so", " ", "far"}], " ", "*)"}], "\n",
            "\t\t\t", 
           RowBox[{
            RowBox[{"PassiveNoise", " ", "\[Rule]", " ", 
             RowBox[{"Circuit", "[", " ", 
              RowBox[{
               SubscriptBox["Deph", "q"], "[", " ", 
               RowBox[{"numGatesOnQubit", "\[LeftDoubleBracket]", 
                RowBox[{"q", "+", "1"}], "\[RightDoubleBracket]"}], " ", 
               "]"}], " ", "]"}]}], ",", "\n", "\t\t\t", 
            RowBox[{"UpdateVariables", " ", "\[Rule]", " ", 
             RowBox[{"Function", "[", "\n", "\t\t\t\t", 
              RowBox[{"totalIdleTime", " ", "+=", " ", "\[CapitalDelta]t"}], 
              "\n", "\t\t\t", "]"}]}]}], "\n", "\t\t", "|>"}]}], "\n", "\t", 
         "}"}]}]}], "\n", "|>"}]}], "]"}]}], ";"}]], "Code",
