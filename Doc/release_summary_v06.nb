  FormBox[
   RowBox[{"R", "[", 
    RowBox[{"\[Pi]", ",", " ", 
     RowBox[{
      SubscriptBox["Z", "1"], 
      SubscriptBox["Z", "2"]}]}], "]"}], TraditionalForm]],
  FontWeight->"Bold",ExpressionUUID->"924a5ffa-c008-4914-98e6-487e33e45c11"],
