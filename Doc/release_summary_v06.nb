Cell[TextData[{
 "Notice too that ",
 Cell[BoxData[
  FormBox[
   RowBox[{"R", "[", 
    RowBox[{"\[Pi]", ",", " ", 
     RowBox[{
      SubscriptBox["Z", "1"], 
      SubscriptBox["Z", "2"]}]}], "]"}], TraditionalForm]],
  FontWeight->"Bold",ExpressionUUID->"39360519-6f83-49b9-a3cc-3f40e176b7d0"],
